{
  "name": "hackathon-adoptaunjunior-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node server/app.ts",
    "seed": "ts-node server/seed-sql.ts",
    "build": "rimraf dist && tsc",
    "dev": "nodemon --exec npx ts-node server/app.ts",
    "seed": "npx prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.18.0",
    "@types/morgan": "^1.9.9",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.1",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@types/node": "^22.7.9",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "ts-node server/seed.ts"
  },
  "nodemonConfig": {
    "exec": "ts-node server/app.ts",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ext": "ts,js",
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
